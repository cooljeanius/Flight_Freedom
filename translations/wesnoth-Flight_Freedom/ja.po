#: Flight_Freedom/units/elementals/Fire_God.cfg:121
msgid "fire breath"
msgstr "火炎のブレス"

#. [variation]
#: Flight_Freedom/units/drakes/Chieftain.cfg:231
msgid "Armed Drake Chieftain"
msgstr "武装ドレークの族長"

#. [variation]
#: Flight_Freedom/units/drakes/Chieftain.cfg:232
msgid "Drake Guerilla Chieftain"
msgstr "ドレークゲリラの族長"

