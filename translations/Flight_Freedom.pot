msgid "The Drake Escape"
msgstr ""

#. [side]: type=General, id=Hago
#: Flight_Freedom/scenarios/09_Drake_Escape.cfg:43
msgid "Hago"
msgstr ""

#. [side]: type=Orcish Warlord, id=Dredgno
#: Flight_Freedom/scenarios/09_Drake_Escape.cfg:58
